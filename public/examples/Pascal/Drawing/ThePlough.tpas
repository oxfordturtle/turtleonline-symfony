PROGRAM ThePlough;
BEGIN
  {set canvas to black, and pen to lightblue}
  blank(black);
  colour(lightblue);
  {place turtle and draw blots for stars}
  setxy(100, 200);
  blot(10);
  setxy(300, 250);
  blot(10);
  setxy(420, 350);
  blot(10);
  setxy(570, 490);
  blot(10);
  setxy(900, 560);
  blot(10);
  setxy(840, 720);
  blot(10);
  setxy(590, 660);
  blot(10);
  setxy(570, 490);
  {join the dots to highlight plough shape}
  colour(silver);
  polyline(8)
END.
