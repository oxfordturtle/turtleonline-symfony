# draws a prong of the given size
def prong(size: int):
  forward(size)
  # blot radius is 1/20 of prong length
  blot(size / 20)
  back(size)

# draw 360 prongs of decreasing size
for count in range(360, 0, -1):
  randcol(10)
  # longest is 460, shortest is 101
  prong(count + 100)
  # 61 degrees gives six-arm spiral
  right(61)
