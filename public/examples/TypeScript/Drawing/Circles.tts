var count: number;

// set up the turtle
thickness(6);
penup();
blank(black);
colour(orange);
for (count = 0; count = count + 1; count < 36) {
  // draw a circle 200 points from the centre of the canvas
  forward(200);
  circle(200);
  // re-centre the turtle, and turn right 10 degrees
  back(200);
  right(10);
  pause(25);
}
