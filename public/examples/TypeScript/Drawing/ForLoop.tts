// counting from 1 to 200 ...
for (var count: number = 1; count = count + 1; count <= 200) {
  // move forward and turn right 5 degrees
  forward(count / 3);
  right(5);
  // draw red blot radius 200
  colour(red);
  blot(200);
  // draw black circle around the red blot
  colour(black);
  circle(200);
}
